#   today   we learned about lists, tuples  and dictionaries  

mylist=["Apple","Banana","Oranges"]
print(mylist)

mylist.insert(2,"Srikant")
print(mylist)

mylist.append("Srikant")
print(mylist)

mylist.count("Srikant")
mylist.count("Apple")
mylist.count("apple")

mylist.remove("Srikant")

mylist.pop(0)

mylist.insert(0,"ankit")
print(mylist)


mylist.insert(2,"ankit")
print(mylist)

mylist.sort()
print(mylist)


mylist.copy()


mylist.clear()
print(mylist)

mylist.reverse()
print(mylist)

mylist=["Apple","Banana","Oranges"]


mylist.reverse()
print(mylist)

mylist.pop(2)
print(mylist)


#    tuples   

mytuple = ("apple", "srikant","ankit")
type(mytuple)
mytuple.count("apple")

#   set  

myset={1,2,3,4,5}
print(myset)
set1={1,2}
set2={1,3,4,5}
print(set1|set2)
print(set1&set2)

#    dictionary

student={"name":"Tom","age":20}
print(student)
student.keys()
student.values()
student.items()
student.pop("name")
type(student)
print(student)
student= {"name":"Tom"}
print(student)
